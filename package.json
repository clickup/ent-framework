{
  "name": "@clickup/ent-framework",
  "description": "A PostgreSQL graph database alike library with micro-sharding and row-level security",
  "version": "2.10.296",
  "license": "MIT",
  "keywords": [
    "postgresql",
    "postgres",
    "orm",
    "sql",
    "replication lag",
    "cluster",
    "sharding",
    "micro-sharding",
    "microsharding",
    "jit"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./pg": "./pg.js"
  },
  "scripts": {
    "barrel": "barrelsby --directory=./src/abstract --directory=./src/ent --directory=./src/helpers --directory=./src/pg --delete --exclude=__ --exclude=internal --name=index",
    "build": "$npm_execpath run barrel && tsc",
    "dev": "$npm_execpath run barrel && tsc --watch --preserveWatchOutput",
    "lint": "eslint . --ext .ts --cache --cache-location dist/.eslintcache",
    "test": "set -e; bash internal/docker-compose-up.sh; export PGHOST=127.0.0.1 PGPORT=54832 PGDATABASE=postgres PGUSER=postgres PGPASSWORD=postgres; bash ./internal/init-db.sh; jest",
    "psql": "PGHOST=127.0.0.1 PGPORT=54832 PGDATABASE=postgres PGUSER=postgres PGPASSWORD=postgres psql",
    "docs": "rm -rf docs && typedoc --plugin typedoc-plugin-markdown --plugin typedoc-plugin-merge-modules && sed -i '' -E 's#packages/[^/]+/##g' $(find docs -type f -name '*.md')",
    "clean": "rm -rf dist yarn.lock package-lock.json pnpm-lock.yaml *.log",
    "copy-package-to-public-dir": "copy-package-to-public-dir.sh",
    "backport-package-from-public-dir": "backport-package-from-public-dir.sh",
    "deploy": "npm run build && npm run lint && npm run test && npm publish --access=public"
  },
  "dependencies": {
    "delay": "^4.4.1",
    "fast-typescript-memoize": "^1.0.2",
    "lodash": "^4.17.21",
    "object-hash": "^2.2.0",
    "p-defer": "^3.0.0",
    "p-timeout": "^3.2.0",
    "pg": "^8.7.1",
    "quick-lru": "^5.1.1"
  },
  "devDependencies": {
    "@clickup/pg-id": "^2.10.292",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.175",
    "@types/node": "^20.4.1",
    "@types/object-hash": "^2.2.1",
    "@types/pg": "^8.6.1",
    "@types/uniqid": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "barrelsby": "^2.8.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-typescript-enum": "^2.1.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.7.0",
    "prettier": "3.2.1",
    "streaming-iterables": "^7.1.0",
    "ts-jest": "^29.1.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typedoc-plugin-merge-modules": "^5.1.0",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "wait-for-expect": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/clickup/ent-framework"
  }
}
