{
  "name": "@clickup/ent-framework",
  "description": "A PostgreSQL graph database alike library with micro-sharding and row-level security",
  "version": "2.12.0",
  "license": "MIT",
  "keywords": [
    "postgresql",
    "postgres",
    "orm",
    "sql",
    "replication lag",
    "cluster",
    "sharding",
    "micro-sharding",
    "microsharding",
    "jit"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./pg": "./pg.js"
  },
  "scripts": {
    "barrel": "barrelsby -c barrelsby.json",
    "build": "$npm_execpath run barrel && tsc",
    "dev": "$npm_execpath run barrel && tsc --watch --preserveWatchOutput",
    "lint": "bash internal/lint.sh",
    "test": "PGPORT=54832 bash internal/with-docker-compose-up.postgres.sh 'bash ./internal/init-db.sh && jest'",
    "psql": "PGPORT=54832 bash internal/with-docker-compose-up.postgres.sh psql",
    "docs": "bash internal/docs.sh",
    "clean": "bash internal/clean.sh",
    "copy-package-to-public-dir": "copy-package-to-public-dir.sh",
    "backport-package-from-public-dir": "backport-package-from-public-dir.sh",
    "deploy": "bash internal/deploy.sh"
  },
  "dependencies": {
    "delay": "^4.4.1",
    "fast-typescript-memoize": "^1.2.1",
    "lodash": "^4.17.21",
    "object-hash": "^2.2.0",
    "p-defer": "^3.0.0",
    "p-timeout": "^3.2.0",
    "pg": "^8.7.1",
    "quick-lru": "^5.1.1"
  },
  "devDependencies": {
    "@clickup/pg-id": "^2.10.292",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.175",
    "@types/node": "^20.4.1",
    "@types/object-hash": "^2.2.1",
    "@types/pg": "^8.6.1",
    "@types/uniqid": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "barrelsby": "^2.8.1",
    "chalk": "^4.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-typescript-enum": "^2.1.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.7.0",
    "prettier": "3.2.1",
    "streaming-iterables": "^7.1.0",
    "table": "^6.8.0",
    "ts-jest": "^29.1.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typedoc-plugin-merge-modules": "^5.1.0",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "wait-for-expect": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/clickup/ent-framework"
  }
}
